{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "noFallthroughCasesInSwitch": true,
      "baseUrl": "./src",
      "paths": {
        "@services": ["./services"],
        "@components": ["./components"],
        "@utils": ["./utils"],
        "@hooks": ["./hooks"],
        "@types": ["./types"],
        "@consts": ["./consts"],
        "@assets": ["./assets/"]
      }
    },
    "exclude": ["node_modules", "dist", "coverage"],
    "types": ["typePatches"],
    "include": ["src", "custom.d.ts", "node_modules/@types"],
    "resolutions": {
      "@types/react": "18.0.21"
    }
  }
  